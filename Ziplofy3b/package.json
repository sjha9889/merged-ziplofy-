{
  "name": "ziplofy3b",
  "version": "1.0.0",
  "description": "Express TypeScript application",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "start": "cross-env DOTENV_CONFIG_PATH=.env.production NODE_ENV=production node build/index.js",
    "dev": "cross-env DOTENV_CONFIG_PATH=.env.development NODE_ENV=development concurrently \"tsc -w\" \"nodemon build/index.js\"",
    "clean": "rimraf build",
    "seed:variants": "node build/seed/variants/seed.variant.js",
    "seed:packaging": "node build/seed/packaging/seed.packaging.js",
    "seed:categories": "node build/seed/categories/seed.categories.js",
    "seed:countries": "node build/seed/seed.countries.js",
    "seed:triggers": "node build/seed/seed.triggers.js",
    "seed:actions": "node build/seed/seed.actions.js",
    "seed:notification-categories": "node build/seed/notifications/seed.notification-categories.js",
    "seed:notification-options": "node build/seed/notifications/seed.notification-options.js",
    "seed:permissions": "node build/seed/permissions/seed.permissions.js",
    "seed:states": "node build/seed/seed.states.js",
    "seed:tax-defaults-india": "node build/seed/seed.tax-defaults-india.js",
    "seed:country-tax": "node build/seed/seed.country-tax.js"
  },
  "keywords": [
    "express",
    "typescript",
    "node"
  ],
  "author": "",
  "license": "ISC",

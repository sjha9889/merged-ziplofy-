    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "extract-zip": "^2.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "nodemon": "^3.1.10",
    "bullmq": "^5.20.1",
    "ioredis": "^5.4.1",
    "socket.io": "^4.8.1",
    "ts-node": "^10.9.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.0",
    "@types/nodemailer": "^7.0.1",
    "concurrently": "^8.2.2",
